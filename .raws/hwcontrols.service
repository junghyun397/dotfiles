[Unit]
Description=AIO/LED initialize service

[Service]
Type=oneshot
ExecStart=sleep 10
ExecStart=liquidctl -m Hydro initialize --pump-mode quiet
ExecStart=liquidctl -m Hydro set fan speed 40 10 50 20 60 30 65 40
ExecStart=liquidctl -m Hydro set fan1 speed 40 10 50 20 60 30 65 40
ExecStart=openrgb --config / --profile /home/junghyun397/.config/OpenRGB/blue.orp

[Install]
WantedBy=default.target
